{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@fixture": ["src/fixture"]
    },
    "module": "CommonJS",
    "moduleResolution": "Node",
    "target": "ES2019",
    "lib": [
      "ESNext"
    ],
    "outDir": "./dist",
    "sourceMap": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "alwaysStrict": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitOverride": true,
  },
  "exclude": [
    "./dist",
    "./node_modules"
  ]
}
